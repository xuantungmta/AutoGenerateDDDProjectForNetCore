{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\SQLEXPRESS;Database=[$database_name];User Id=[$sql_username];Password=[$sql_password];Integrated Security=SSPI;MultipleActiveResultSets=True;"
  },
  "Authentication": {
    "IdentityServer": {
      "TokenAuthority": "https://localhost:44328/connect/token",
      "Authority": "https://localhost:44328"
    }
  }
}
